{
	"name": "eduprime.backend",
	"version": "0.0.1",
	"description": "api for eduprime backend",
	"author": "@hermesalvesbr",
	"private": true,
	"license": "MIT",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
